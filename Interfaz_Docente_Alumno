    private void btn_salirActionPerformed(java.awt.event.ActionEvent evt) {                                          
        int r = JOptionPane.showOptionDialog(this, "¿Estas seguro que deseas salir...?",
                "Comerciantes Unidos", JOptionPane.YES_NO_OPTION,
                JOptionPane.QUESTION_MESSAGE, null, new Object[]{"Si Salgo", "No Salgo"}, "No Salgo");
        if (r == 0) {
            System.exit(0);
        }
    }                                         

    private void btn_calcularActionPerformed(java.awt.event.ActionEvent evt) {                                             
        String nombre, apellido;
        int edad, n1, n2, n3, horas;
        float tarifa;
        nombre = txt_nombre.getText();
        apellido = txt_apellidos.getText();
        edad = Integer.parseInt(txt_edad.getText());
        if (chkAlumno.isSelected()) {
            n1 = Integer.parseInt(txtNota01.getText());
            n2 = Integer.parseInt(txtNota02.getText());
            n3 = Integer.parseInt(txtNota03.getText());
            objAlumno.setNombres(nombre);
            objAlumno.setApellidos(apellido);
            objAlumno.setEdad(edad);
            objAlumno.setNota1(n1);
            objAlumno.setNota2(n2);
            objAlumno.setNota3(n3);
            this.txtNota01.setEnabled(true);
            this.txtNota02.setEnabled(true);
            this.txtNota03.setEnabled(true);
            this.txtHoras.setEnabled(false);
            this.txtTarifa.setEnabled(false);
            txtResultado.setText("Datos Personales");
            txtResultado.append("\n===============");
            txtResultado.append("\nNombres : " + objAlumno.getNombres());
            txtResultado.append("\nApellidos : " + objAlumno.getApellidos());
            txtResultado.append("\nEdad : " + String.valueOf(objAlumno.getEdad()));
            txtResultado.append("\nNota 01 : " + String.valueOf(objAlumno.getNota1()));
            txtResultado.append("\nNota 02 : " + String.valueOf(objAlumno.getNota2()));
            txtResultado.append("\nNota 03 : " + String.valueOf(objAlumno.getNota3()));
            txtResultado.append("\nPromedio : " + String.valueOf(objAlumno.mostrarPromedio()));
            txtResultado.append("\ne - mail : " + String.valueOf(objAlumno.generaCorreo()));
        } else {
            horas = Integer.parseInt(txtHoras.getText());
            tarifa = Float.parseFloat(txtTarifa.getText());
            objDocente.setNombres(nombre);
            objDocente.setApellidos(apellido);
            objDocente.setEdad(edad);
            objDocente.setHoras(horas);
            objDocente.setTarifa((int) tarifa);
            this.txtHoras.setEnabled(true);
            this.txtTarifa.setEnabled(true);
            this.txtNota01.setEnabled(false);
            this.txtNota02.setEnabled(false);
            this.txtNota03.setEnabled(false);
            txtResultado.setText("Datos Personales");
            txtResultado.append("\n===============");
            txtResultado.append("\nNombres : " + objDocente.getNombres());
            txtResultado.append("\nApellidos : " + objDocente.getApellidos());
            txtResultado.append("\nEdad : " + String.valueOf(objDocente.getEdad()));
            txtResultado.append("\nSueldo : " + String.valueOf(objDocente.mostrarCalcularSueldo()));
            txtResultado.append("\ne - mail : " + String.valueOf(objDocente.generaCorreo()));
        }

    }                                            

    private void btn_nuevoActionPerformed(java.awt.event.ActionEvent evt) {                                          
        txtTarifa.setText("");
        txtHoras.setText("");
        txtNota01.setText("");
        txtNota02.setText("");
        txtNota03.setText("");
        txt_nombre.setText("");
        txt_apellidos.setText("");
        txt_edad.setText("");
        txtResultado.setText("");
        chkAlumno.isSelected();
        txtTarifa.requestFocus();
        btn_nuevo.setEnabled(true);
        chkProfesor.isSelected();
        btn_calcular.setEnabled(true);
        if (chkAlumno.isSelected()) {
            this.txtHoras.setEnabled(false);
            this.txtTarifa.setEnabled(false);
            this.txtNota01.setEnabled(true);
            this.txtNota02.setEnabled(true);
            this.txtNota03.setEnabled(true);
        } else {
            this.txtHoras.setEnabled(true);
            this.txtTarifa.setEnabled(true);
            this.txtNota01.setEnabled(false);
            this.txtNota02.setEnabled(false);
            this.txtNota03.setEnabled(false);
        }

    }                                         

